{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "npm run build:server && node dist/server/index.js",
    "build": "npm run build:all",
    "build:all": "npm run build:next && npm run build:server",
    "build:next": "next build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "NODE_ENV=production concurrently \"next start\" \"npm run start:server\"",
    "start:server": "node dist/server/index.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/http-proxy-middleware": "^0.19.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "geist": "^1.3.1",
    "http-proxy-middleware": "^3.0.3",
    "next": "^15.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.5.14",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
